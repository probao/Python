biggest = 0
i = 0
while i < len(num) - 12:
        one = int(num[i])
        two = int(num[i+1])
        thre = int(num[i+2])
        fou = int(num[i+3])
        fiv = int(num[i+4])
        six = int(num[i+5])
        svn = int(num[i+6])
        eight = int(num[i+7])
        nine = int(num[i+8])
        ten = int(num[i+9])
        eleven = int(num[i+10])
        twelve = int(num[i+11])
        thirteen = int(num[i+12])
        product = one*two*thre*fou*fiv*six*svn*eight*nine*ten*eleven*twelve*thirteen
        if product  > biggest:
                numbers = []
                biggest = product
                numbers = [one, two, thre, fou, fiv, six, svn, eight, nine, ten, eleven, twelve, thirteen]
        i += 1

print(biggest)
print(numbers)

