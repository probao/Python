i = len(A) - 2
j = 0

while i >= 0:
        for j in range(len(A[i])):
                number_origin = A[i][j]
                number_1 = A[i+1][j]
                number_2 = A[i+1][j+1]
                larger = max(number_1, number_2)
                A[i][j] = number_origin + larger
        i = i -1

print A[0][0]



