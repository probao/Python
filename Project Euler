        Tri_num.append(n*(n+1)/2)
        Pen_num.append(n*(3*n-1)/2)
        Hex_num.append(n*(2*n - 1))
        if Tri_num[n-144] in Pen_num and Tri_num[n-144] in Hex_num:
                print Tri_num[n-144]
                num_tph += 1
        n += 1
