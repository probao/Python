def check_prime(number):
        if number != 1:
                for factor in range(2, number):
                        if number % factor == 0:
                                return False
        else:
                return False
        return True

if __name__ == '__main__':
        n = int(input('Which prime do you want: '))
        prime = []
        i = 0
        number = 1
        while i < n:
                if check_prime(number):
                        prime.append(number)
                        i += 1
                number += 1
        print(prime[-1])
        #print(prime)
